{
    "log": {
        "loglevel": "debug",
	"access": "/tmp/access.log",
	"error": "/tmp/error.log"
    },
    "dns": {
	"queryStrategy": "UseIPv4",
	"disableCache": true,
	"disableFallbackIfMatch": false
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "geosite:cn"
                ],
                "outboundTag": "block"
            }
	]
    },
    "inbounds": [
        {
            "port": 1443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "f8ab4118-355e-4404-814e-b3229173fb29",
                        "level": 0
                    }
                ],
                "decryption": "none",
                "fallbacks": [
		    {
		    	"dest": 80
		    },
                    {
                        "alpn": "http/1.1",
                        "dest": 80
                    },
                    {
                        "alpn": "h2",
                        "dest": 80,
                        "xver": 1
                    },
                    {
                        "alpn": "h3",
                        "dest": 80,
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "ipad.imrcrab.com",
                    "alpn": [
                        "h2",
                        "h3"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/letsencrypt/live/ipad.imrcrab.com/fullchain.pem",
                            "keyFile": "/etc/letsencrypt/live/ipad.imrcrab.com/privkey.pem"
                        }
                    ]
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            }
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ]
}
