{
    "log": {
        "loglevel": "warning",
        "access": "/tmp/xtls_access.log",
        "error": "/tmp/xtls_error.log"
    },
    "dns": {
        "servers": [
            "1.1.1.1",
            "8.8.8.8"
        ],
        "queryStrategy": "UseIPv4",
        "disableCache": true,
        "disableFallbackIfMatch": false
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 1443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "flow": "xtls-rprx-direct",
                        "level": 0,
                        "id": "f8ab4118-355e-4404-814e-b3229173fb29"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "xtls",
                "xtlsSettings": {
                    "serverName": "ali.imrcrab.com",
                    "rejectUnknownSni": true,
                    "alpn": [
                        "h3","h2"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/root/ali.imrcrab.com/ali.imrcrab.com_bundle.pem",
                            "keyFile": "/root/ali.imrcrab.com/ali.imrcrab.com.key"
                        }
                    ]
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "tag": "direct"
        },
        {
            "tag": "blocked",
            "protocol": "blackhole"
        }
    ],
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "ip": [
                    "1.1.1.1",
                    "8.8.8.8"
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:cn"
                ],
                "outboundTag": "blocked"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:cn",
                    "geosite:category-games@cn"
                ],
                "outboundTag": "blocked"
            }
        ]
    }
}
